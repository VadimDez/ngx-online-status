  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test --watch=false",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test:watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "docs": "ng build --configuration production --output-path docs --base-href ngx-online-status && cp ./docs/index.html ./docs/404.html",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VadimDez/ngx-online-status.git"
